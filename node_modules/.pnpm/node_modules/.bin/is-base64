#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/t/School Repos/BYU/AEGIS/node_modules/.pnpm/is-base64@1.1.0/node_modules/is-base64/bin/node_modules:/mnt/t/School Repos/BYU/AEGIS/node_modules/.pnpm/is-base64@1.1.0/node_modules/is-base64/node_modules:/mnt/t/School Repos/BYU/AEGIS/node_modules/.pnpm/is-base64@1.1.0/node_modules:/mnt/t/School Repos/BYU/AEGIS/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/t/School Repos/BYU/AEGIS/node_modules/.pnpm/is-base64@1.1.0/node_modules/is-base64/bin/node_modules:/mnt/t/School Repos/BYU/AEGIS/node_modules/.pnpm/is-base64@1.1.0/node_modules/is-base64/node_modules:/mnt/t/School Repos/BYU/AEGIS/node_modules/.pnpm/is-base64@1.1.0/node_modules:/mnt/t/School Repos/BYU/AEGIS/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../is-base64/bin/is-base64" "$@"
else
  exec node  "$basedir/../is-base64/bin/is-base64" "$@"
fi
